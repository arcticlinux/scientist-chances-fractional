{
  "name": "treetime-ca/scientist-chances-fractional",
  "description": "A fractional chance class for daylerees/scientist",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Best",
      "email": "mbest@treetime.ca"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TreeTime-ca/scientist.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Scientist\\Chances\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.3",
    "daylerees/scientist": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "friendsofphp/php-cs-fixer": "2.*",
    "phpcompatibility/php-compatibility": "*",
    "phpmd/phpmd": "2.*",
    "phpstan/phpstan": "*",
    "squizlabs/php_codesniffer": "3.*",
    "vimeo/psalm": "*"
  }
}
