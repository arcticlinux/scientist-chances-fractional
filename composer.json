{
  "name": "arcticlinux/scientist-chances-fractional",
  "description": "A fractional chance class for daylerees/scientist",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Best",
      "email": "mbest@arcticlinux.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Scientist\\Chances\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": ">=8.1",
    "daylerees/scientist": "^2.1.1"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "friendsofphp/php-cs-fixer": "2.*",
    "phpcompatibility/php-compatibility": "*",
    "phpmd/phpmd": "2.*",
    "phpstan/phpstan": "*",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "3.*",
    "vimeo/psalm": "*"
  }
}
